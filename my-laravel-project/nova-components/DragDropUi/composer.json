{
    "name": "david-malevsky/drag-drop-ui",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "DavidMalevsky\\DragDropUi\\": "nova-components/DragDropUi/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidMalevsky\\DragDropUi\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
